{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "com.flexso.htf2022",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "EventService": {
                "uri": "/IOTAS-COMPOSITE-EVENTS-ODATA/CompositeEvents/EventType/v1/com.sap.appiot.eventtypes:StandardEventType",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/EventsMetadata/metadata.xml"
                }
            },
            "mainService": {
                "uri": "/IOTAS-ADVANCEDLIST-THING-ODATA/CompositeThings/v1/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "Thing_Service1": {
                "uri": "/IOTAS-DETAILS-THING-ODATA/CompositeThings/ThingType/v1/iotae.leonardoflexso.be.miko.piloot:MM_TT_GERHARDT_1",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/Thing_Metadata1/metadata.xml"
                }
            },
            "Thing_Service2": {
                "uri": "/IOTAS-DETAILS-THING-ODATA/CompositeThings/ThingType/v1/iotae.flexso.be.miko.piloot:MM_TT_GERHARDT_1",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/Thing_Metadata2/metadata.xml"
                }
            },
            "Analytics_Normal_Service": {
                "uri": "/IOTAS-ANALYTICS-THING-ODATA/iotae.flexso.be.miko.piloot:MM_PSET_NormalCounter",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/Thing_Metadata2/metadata.xml"
                }
            },
            "Analytics_Test_Service": {
                "uri": "/IOTAS-ANALYTICS-THING-ODATA/iotae.flexso.be.miko.piloot:MM_PSET_TestCounter",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/Thing_Metadata2/metadata.xml"
                }
            },
            "Analytics_Key_Service": {
                "uri": "/IOTAS-ANALYTICS-THING-ODATA/iotae.flexso.be.miko.piloot:MM_PSET_KeyCounter",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/Thing_Metadata2/metadata.xml"
                }
            },
            "Analytics_Total_Service": {
                "uri": "/IOTAS-ANALYTICS-THING-ODATA/iotae.flexso.be.miko.piloot:MM_PSET_TotalCounter",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/Thing_Metadata2/metadata.xml"
                }
            },
            "UserService": {
                "uri": "/business-partner",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        },
        "title": "{{appTitle}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "ui.appbuilder.iotappmtatemplate",
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": {
            "viewName": "com.flexso.htf2022.view.App",
            "type": "XML"
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ui.vbm": {},
				"sap.ui.iot": {}
			}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.flexso.htf2022.i18n.i18n"
                }
            },
            "events": {
                "dataSource": "EventService",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline",
                    "headers": {
                        "sap-iot-eventtype": "com.sap.appiot.eventtypes:StandardEventType",
                        "sap-iot-pst": "iotae.flexso.be.miko.piloot:DefaultImagePropertySet,iotae.flexso.be.miko.piloot:MM_PSET_Button_Names,iotae.flexso.be.miko.piloot:MM_PSET_Error,iotae.flexso.be.miko.piloot:MM_PSET_BASIC,iotae.flexso.be.miko.piloot:MM_PSET_KeyCounter,iotae.flexso.be.miko.piloot:MM_PSET_NormalCounter,iotae.flexso.be.miko.piloot:MM_PSET_Masterdata,iotae.flexso.be.miko.piloot:MM_PSET_TestCounter,iotae.flexso.be.miko.piloot:MM_PSET_TotalCounter,iotae.flexso.be.miko.piloot:MM_PSET_Total,iotae.flexso.be.miko.piloot:MM_PSET_Version"
                    }
                }
            },
            "thingModel1": {
                "dataSource": "Thing_Service1",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            },
            "thingModel2": {
                "dataSource": "Thing_Service2",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            },
            "analyticsNormalModel": {
                "dataSource": "Analytics_Normal_Service",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            },
            "analyticsKeyModel": {
                "dataSource": "Analytics_Key_Service",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            },
            "analyticsTestModel": {
                "dataSource": "Analytics_Test_Service",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            },
            "analyticsTotalModel": {
                "dataSource": "Analytics_Total_Service",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            },
            "userModel": {
                "dataSource": "UserService",
                "preload": true,
                "settings": {
                    "useBatch": false,
                    "defaultCountMode": "Inline"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "com.flexso.htf2022.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": "notFound"
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "landingpage",
                    "target": "landingpage"
                },
                {
                    "pattern": "analysispage/{thingId}/:navFrom:/:headerTitle:/:subHeaderTitle:/:mpPath:",
                    "name": "analysispage",
                    "target": "analysispage"
                },
                {
                    "pattern": "btnanalysispage/{thingId}/{thingType}/{button}",
                    "name": "btnanalysispage",
                    "target": "btnanalysispage"
                },
                {
                    "pattern": "thingpage/{thingId}/{thingType}/:highSeverity:/:mediumSeverity:/:lowSeverity:",
                    "name": "thingpage",
                    "target": "thingpage"
                },
                {
                    "pattern": "thinglistpage",
                    "name": "thinglistpage",
                    "target": "thinglistpage"
                }
            ],
            "targets": {
                "landingpage": {
                    "viewName": "LandingPage",
                    "viewId": "landingpage",
                    "viewLevel": 1
                },
                "analysispage": {
                    "viewName": "AnalysisPage",
                    "viewId": "analysispage",
                    "viewLevel": 1
                },
                "btnanalysispage": {
                    "viewName": "ButtonAnalysisPage",
                    "viewId": "btnanalysispage",
                    "viewLevel": 1
                },
                "thingpage": {
                    "viewName": "ThingPage",
                    "viewId": "thingpage",
                    "viewLevel": 1
                },
                "thinglistpage": {
                    "viewName": "ThingListPage",
                    "viewId": "thinglistpage",
                    "viewLevel": 1
                },
                "objectNotFound": {
                    "viewName": "ObjectNotFound",
                    "viewId": "objectNotFound"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewId": "notFound"
                }
            }
        }
    }
}